{
  "name": "orlovtech/short-link",
  "description": "Link shortener package for Laravel",
  "type": "library",
  "homepage": "https://github.com/orlovtech/short-link",
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin Orlov",
      "email": "orlovkn.job@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/container": "^8.0|^9.0|^10.0",
    "illuminate/database": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "OrlovTech\\ShortLink\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrlovTech\\ShortLink\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OrlovTech\\ShortLink\\Providers\\ShortLinkServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "pint --test",
    "lint-fix": "pint"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
